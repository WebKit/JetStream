{
  "name": "typescript-compile",
  "version": "1.0.0",
  "description": "A demo project to evaluate TypeScript performance.",
  "main": "index.js",
  "scripts": {
    "test": "node ./src/benchmark-node.cjs",
    "clean": "rm -rf dist/* && rm -rf src/gen/*.cjs",
    "import": "node ./build/import-src-project.js",
    "prebuild": "npm run clean  && npm run import",
    "build": "webpack && npm run postbuild",
    "build:dev": "npm run prebuild && webpack --mode development && npm run postbuild",
    "postbuild": "node ./build/postbuild.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/node": "^24.2.0",
    "ansi-styles": "^6.2.1",
    "glob": "^11.0.3",
    "path-browserify": "^1.0.1",
    "typescript": "^5.9.2",
    "undici-types": "^7.13.0",
    "util": "^0.12.5",
    "webpack": "^5.101.0",
    "webpack-cli": "^6.0.1"
  }
}
