{
  "name": "web-tooling-benchmark",
  "description": "JavaScript benchmark for common web developer workloads",
  "main": "src/cli.js",
  "scripts": {
    "benchmark": "node dist/cli.js",
    "build:terser-bundled": "node node_modules/terser/bin/uglifyjs -b preamble=\"'const Terser = module.exports = {};'\" --self > build/terser-bundled.js",
    "build": "webpack",
    "test": "jest",
    "update-lock": "npm install --package-lock"
  },
  "lint-staged": {
    "src/**/*.css": [
      "prettier --write",
      "git add"
    ],
    "src/**/*.js": [
      "prettier --write",
      "git add"
    ],
    "webpack.config.js": [
      "prettier --write",
      "git add"
    ]
  },
  "license": "BSD-3-Clause",
  "dependencies": {
    "@babel/standalone": "7.28.3",
    "acorn": "8.15.0",
    "autoprefixer": "10.4.21",
    "babylon": "7.0.0-beta.32",
    "chai": "5.3.1",
    "eslint": "^9.33.0",
    "espree": "10.4.0",
    "esprima-next": "^6.0.3",
    "lebab": "3.2.7",
    "postcss": "8.5.6",
    "postcss-nested": "7.0.2",
    "prettier": "3.6.2",
    "rollup": "^4.46.3",
    "source-map": "0.7.6",
    "terser": "5.43.1",
    "typescript": "5.9.2",
    "virtualfs": "^2.2.0"
  },
  "devDependencies": {
    "copy-webpack-plugin": "^13.0.1",
    "jest": "^30.0.5",
    "lint-staged": "^16.1.5",
    "os-browserify": "^0.3.0",
    "webpack": "^5.101.3"
  }
}
